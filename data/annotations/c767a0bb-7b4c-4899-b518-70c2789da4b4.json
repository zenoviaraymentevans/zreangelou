    },
    {
      "start_time": 150,
      "end_time": 151,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I rise"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
      ],
      "uuid": "6f0b08fc-3f65-49e3-8c81-6ab107fbbdc7"
    },
    {
      "start_time": 152,
      "end_time": 155,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Bringing the gifts that my ancestors gave,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
      ],
      "uuid": "9682d1d5-bb3c-4893-bd15-1c24b86c448d"
    },
    {
      "start_time": 156,
      "end_time": 160,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I am the dream and the hope of the slave."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        }
      ],
      "uuid": "c0b130e9-c2b6-42c6-be02-913f64dacccb"
    },

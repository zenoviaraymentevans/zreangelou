      "start_time": 161,
      "end_time": 162,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And so"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        }
      ],
      "uuid": "dc8bec35-3adf-439d-ba83-bd6a28e61520"
    },
    {
      "start_time": 163,
      "end_time": 164,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "naturally"
            }
          ]
        }
      ],
      "tags": [
        {
